{
  "name": "server",
  "version": "0.1.0",
  "private": true,
  "main": "./src/index.ts",
  "scripts": {
    "build": "tsc",
    "dev": "cross-env NODE_ENV=development nodemon --exec ts-node --files ./src/server.ts",
    "env-check": "ts-node ./src/check-env.ts",
    "regen-types": "npx gen-env-types .env",
    "start": "cross-env NODE_ENV=production node ./build/server.js",
    "lint": "eslint . --ext .ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "db:migration:generate": "bun typeorm migration:generate -d ./src/dataSource.ts",
    "db:migration:run": "bun typeorm migration:run -d ./src/dataSource.ts"
  },
  "dependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/markdown-it": "^13.0.7",
    "@types/markdown-table": "^2.0.0",
    "@types/memory-cache": "^0.2.5",
    "@types/node": "^20.12.3",
    "@types/passport": "^1.0.16",
    "@types/passport-discord": "^0.1.13",
    "@types/passport-oauth2": "^1.4.15",
    "axios": "^1.6.8",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "connect-pg-simple": "^9.0.1",
    "cors": "^2.8.5",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "markdown-it": "^14.1.0",
    "markdown-table": "^2.0.0",
    "memory-cache": "^0.2.0",
    "passport": "^0.7.0",
    "passport-discord": "^0.1.4",
    "passport-oauth2": "^1.8.0",
    "reflect-metadata": "^0.2.2",
    "routing-controllers": "^0.10.4",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.20",
    "typeorm-typedi-extensions": "^0.4.1",
    "typescript": "^5.4.3"
  },
  "devDependencies": {
    "@types/connect-pg-simple": "^7.0.3",
    "cross-env": "^7.0.3",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2"
  }
}
