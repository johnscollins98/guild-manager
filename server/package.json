{
  "name": "server",
  "version": "0.1.0",
  "private": true,
  "main": "./src/index.ts",
  "scripts": {
    "build": "tsc",
    "dev": "cross-env NODE_ENV=development nodemon --exec ts-node --files ./src/server.ts",
    "env-check": "ts-node ./src/check-env.ts",
    "regen-types": "npx gen-env-types .env",
    "start": "cross-env NODE_ENV=production node ./build/server.js",
    "lint": "eslint . --ext .ts",
    "db:migrate": "typeorm-ts-node-commonjs migration:generate -d ./src/dataSource.ts",
    "db:deploy:dev": "typeorm-ts-node-commonjs migration:run -d ./src/dataSource.ts",
    "db:deploy:prod": "typeorm-ts-node-commonjs migration:run -d ./build/dataSource.js",
    "bot:deploy-commands:prod": "node ./build/discord-bot/deploy-commands.js",
    "bot:deploy-commands:dev": "ts-node -r dotenv/config --files  ./src/discord-bot/deploy-commands.ts",
    "predeploy:prod": "npm run db:deploy:prod && npm run bot:deploy-commands:prod",
    "predeploy:dev": "npm run db:deploy:dev && npm run bot:deploy-commands:dev"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "axios-logger": "^2.8.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "connect-pg-simple": "^10.0.0",
    "cors": "^2.8.5",
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "glob": "^11.0.0",
    "markdown-it": "^14.1.0",
    "markdown-table": "^2.0.0",
    "memory-cache": "^0.2.0",
    "passport": "^0.7.0",
    "passport-discord": "^0.1.4",
    "cross-env": "^7.0.3",
    "passport-oauth2": "^1.8.0",
    "reflect-metadata": "^0.2.2",
    "routing-controllers": "^0.10.4",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.20",
    "typeorm-typedi-extensions": "^0.4.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.18.1",
    "@types/markdown-it": "^14.1.2",
    "@types/markdown-table": "^2.0.0",
    "@types/memory-cache": "^0.2.6",
    "@types/node": "^22.10.2",
    "@types/passport": "^1.0.17",
    "@types/passport-discord": "^0.1.14",
    "@types/passport-oauth2": "^1.4.17",
    "@types/connect-pg-simple": "^7.0.3",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
