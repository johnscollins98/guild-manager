{
  "name": "my-guild-manager",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cd Server && yarn start",
    "heroku-prebuild": "cd Server && yarn && cd ../Client && yarn",
    "build": "cd Client && yarn build",
    "dev-server": "cd Server && yarn dev",
    "dev-client": "cd Client && yarn start",
    "dev": "concurrently \"yarn dev-server\" \"set PORT=5001 && yarn dev-client\""
  },
  "devDependencies": {
    "concurrently": "^5.3.0"
  }
}
