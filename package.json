  "license": "MIT",
  "scripts": {
    "postinstall": "yarn --cwd Client && yarn --cwd Server",
    "build": "yarn --cwd Client build",
    "start": "yarn --cwd Server start",
    "dev-server": "yarn --cwd Server dev",
    "dev-client": "yarn --cwd Client start",
    "dev": "concurrently \"yarn dev-server\" \"set PORT=5001 && yarn dev-client\""
